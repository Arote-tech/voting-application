{
  "compilerOptions": {
    "ignoreDeprecations": "6.0",
    "moduleResolution": "Bundler",
    "jsx": "preserve",
    "target": "esnext",
    "paths": {
      "@/*": ["./*"],
      "@/styles/*": ["./styles/*"],
      "@/components/*": ["./components/*"],
      "@/lib/*": ["./lib/*"],
      "@/public/*": ["./public/*"],
      "@/app/*": ["./app/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.js", "**/*.jsx", "**/*.mjs"],
  "exclude": ["node_modules"]
}
